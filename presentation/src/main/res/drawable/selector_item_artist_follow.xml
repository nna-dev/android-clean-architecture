<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?attr/colorRipple">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <corners android:radius="18dp"/>
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#19141414"/>
            <stroke android:width="0.7dp" android:color="#66feffff"/>
            <corners android:radius="18dp"/>
        </shape>
    </item>
</ripple>
