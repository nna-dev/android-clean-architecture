<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/dark_ripple">
    <item android:id="@android:id/mask">
        <color android:color="@android:color/white"/>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#199257df" android:endColor="#009257df" android:angle="0" android:type="linear"/>
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_selected="true" android:drawable="@color/transparent"/>
            <item android:drawable="@android:color/transparent"/>
        </selector>
    </item>
</ripple>
