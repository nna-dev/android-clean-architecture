<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/dark_colorItemDisable"/>
            <corners android:radius="@dimen/vip_text_rounded_radius"/>
        </shape>
    </item>
    <item android:state_selected="true" android:drawable="@drawable/bg_btn_get_vip"/>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent"/>
            <stroke android:width="1dp" android:color="@color/colorTipVipItem"/>
            <corners android:radius="@dimen/vip_text_rounded_radius"/>
        </shape>
    </item>
</selector>
