<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/dark_ripple">
    <item android:id="@android:id/mask">
        <shape android:shape="oval">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_focused="true" android:drawable="@color/dark_ripple"/>
            <item android:drawable="@android:color/transparent"/>
        </selector>
    </item>
</ripple>
