<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="?attr/colorVipGradientStart" android:endColor="?attr/colorVipGradientEnd" android:angle="135" android:centerColor="?attr/colorVipGradientCenter"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="?attr/colorBackground"/>
            <corners android:radius="18dp"/>
        </shape>
    </item>
</layer-list>
