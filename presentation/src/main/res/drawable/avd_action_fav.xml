<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false" android:id="@+id/disable" android:drawable="@drawable/ic_action_fav_disable"/>
    <item android:state_selected="true" android:id="@+id/selected" android:drawable="@drawable/ic_action_fav_full"/>
    <item android:id="@+id/normal" android:drawable="@drawable/ic_action_fav"/>
    <transition android:drawable="@drawable/res_0x7f080004_avd_action_fav__0" android:toId="@+id/selected" android:fromId="@+id/normal"/>
    <transition android:drawable="@drawable/res_0x7f080007_avd_action_fav__3" android:toId="@+id/normal" android:fromId="@+id/selected"/>
    <transition android:drawable="@drawable/res_0x7f08000a_avd_action_fav__6" android:toId="@+id/disable" android:fromId="@+id/normal"/>
    <transition android:drawable="@drawable/res_0x7f08000c_avd_action_fav__8" android:toId="@+id/normal" android:fromId="@+id/disable"/>
</animated-selector>
